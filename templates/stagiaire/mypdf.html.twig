<h1>Certificat de reussite</h1>

    {% for session in stagiaire.sessions %}
        {{session.promotion}}
    {% endfor %}

    <table class="table">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ stagiaire.nom }}</td>
            </tr>
            <tr>
                <th>Prénom</th>
                <td>{{ stagiaire.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ stagiaire.email }}</td>
            </tr>
            <tr>
                <th>Téléphone</th>
                <td>{{ stagiaire.telephone }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ stagiaire.adresse }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ stagiaire.ville }}</td>
            </tr>
            <tr>
                <th>cp</th>
                <td>{{ stagiaire.cp }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ stagiaire.sexe }}</td>
            </tr>
            
        </tbody>
    </table>
